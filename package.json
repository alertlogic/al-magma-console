{
  "name": "magma",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve console --port=8888",
    "console": "nx serve console --port=8888",
    "pattern-library": "nx serve pattern-library --port=4201",
    "build": "nx build",
    "test": "nx test",
    "dev:ssr": "ng run console:serve-ssr",
    "serve:ssr": "node dist/console/server/main.js",
    "build:ssr": "ng build && ng run console:server",
    "prerender": "ng run console:prerender",
    "cypress:e2e": "./scripts/runCypressTests.sh"
  },
  "private": true,
  "buildData": {
    "preapproved": true,
    "rune2eTests": true,
    "cfn": {
      "git": {
        "user": "alertlogic",
        "repo": "cfn-stack-al-console"
      }
    },
    "e2e": {
      "creds": [
        "SECURITY_UI_PASSWORD",
        "CONFIG_UI_PASSWORD",
        "DASHBOARD_UI_PASSWORD"
      ],
      "prSuite": "pullRequest"
    },
    "sourceBase": "dist/console/browser",
    "buildBase": "deployable",
    "targets": {
      "integration": {
        "format": "default",
        "cfDistId": "EQ4529IOIGED8",
        "environment": "integration",
        "url": "console.magma.dev.alertlogic.com"
      },
      "integration_dev": {
        "path": "",
        "format": "archive",
        "environment": "dev"
      },
      "integration_build": {
        "path": "",
        "format": "archive",
        "environment": "prod"
      },
      "staging-test": {
        "bucket": "s3://o3-staging-repo",
        "path": "/versions",
        "format": "archive",
        "environment": "prod"
      },
      "staging": {
        "bucket": "s3://route105.repository",
        "path": "/al-magma",
        "format": "archive",
        "environment": "prod"
      }
    }
  },
